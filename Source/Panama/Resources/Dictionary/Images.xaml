<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Image size for menu items -->
    <!--<sys:Double x:Key="MenuImageXY">24</sys:Double>-->
    <!-- Image size for tool bar buttons -->
    <!--<sys:Double x:Key="ToolBarImageXY">32</sys:Double>-->

    <!--<sys:Double x:Key="ToolBarToggleImageXY">16</sys:Double>-->

    <!-- Images -->
    <BitmapImage x:Key="ImageAccepted" UriSource="/Panama;component/Resources/Images/Image.Accepted.48.png"/>
    <BitmapImage x:Key="ImageAdd" UriSource="/Panama;component/Resources/Images/Image.Add.64.png"/>
    <BitmapImage x:Key="ImageArrowDown" UriSource="/Panama;component/Resources/Images/Image.Arrow.Down.48.png"/>
    <BitmapImage x:Key="ImageArrowLeft" UriSource="/Panama;component/Resources/Images/Image.Arrow.Left.48.png"/>
