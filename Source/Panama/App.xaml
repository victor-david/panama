<Application 
    x:Class="Restless.App.Panama.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:win="clr-namespace:System.Windows;assembly=PresentationFramework"
    xmlns:local="clr-namespace:Restless.App.Panama"
    xmlns:diag="clr-namespace:System.Diagnostics;assembly=WindowsBase">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/App.Brushes.xaml"/>
                <ResourceDictionary Source="Resources/App.Images.xaml"/>
                <ResourceDictionary Source="Resources/App.Controls.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
