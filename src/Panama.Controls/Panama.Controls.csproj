<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1</TargetFrameworks>
    <UseWpf>true</UseWpf>
    <OutputType>Library</OutputType>
    <PackageId>Panama.Controls</PackageId>
    <AssemblyName>Panama.Controls</AssemblyName>
    <RootNamespace>Restless.Panama.Controls</RootNamespace>
    <Description>Provides custom controls for the Panama application.</Description>
    <PackageProjectUrl>https://github.com/victor-david/panama</PackageProjectUrl>
    <RepositoryUrl>https://github.com/victor-david/panama</RepositoryUrl>
    <Authors>Restless Animal Development</Authors>
    <Company>Restless Animal Development</Company>
    <Version>1.0.0</Version>
    <Product>Panama Controls</Product>
    <Title>Panama Controls</Title>
    <PackageLicenseExpression>GPL-3.0-only</PackageLicenseExpression>
    <Copyright>Copyright Â© 2022</Copyright>
    <PackageTags>wpf controls wpf-controls</PackageTags>
    <PackageReleaseNotes></PackageReleaseNotes>
  </PropertyGroup>
  <ItemGroup>
    <Page Update="Themes\Generic\TagSelectorControl.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
    </Page>
  </ItemGroup>
</Project>